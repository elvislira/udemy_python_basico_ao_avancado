   "source": [
    "from functools import reduce\n",
    "\n",
    "numeros = [numero for numero in range(1, 11)]\n",
    "\n",
    "total = reduce(\n",
    "    lambda acumulador, valor: acumulador + valor,\n",
    "    numeros,\n",
    "    0\n",
    ")\n",
    "\n",
    "print(f'{numeros}: Total = {total}')"
   ]
