   "source": [
    "# Generators"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def gerador(n=0, maximo=10):\n",
    "    while True:\n",
    "        yield n\n",
    "        n += 1\n",
    "\n",
    "        if n >= maximo:\n",
    "            return"
   ]
  },
  {
   "cell_type": "code",
