   "source": [
    "def fatorial(numero):\n",
    "    if numero < 0:\n",
    "        return None\n",
    "    \n",
    "    if numero == 0 or numero == 1:\n",
    "        return 1\n",
    "    \n",
    "    return numero * fatorial(numero - 1)"
   ]
  },
